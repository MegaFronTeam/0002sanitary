mixin sPartners(data)
	// start sPartners
	+b.SECTION.sPartners.section#sPartners&attributes(attributes)
		.container
			+b.section-title
				h2 Партнеры
				+e.btn.btn.btn-primary Перейти в раздел
			.sEpidGuide__swiper.swiper
				//- - for (var x = 0; x < 3; x++)
				.swiper-wrapper
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-1.png`)
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-2.png`)
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-3.png`)
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-2.png`)
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-4.png`)
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-1.png`)
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-4.png`)
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-3.png`)
					.swiper-slide
						+e.item(href="#")
							+img(`img/partner-3.png`)
	// end sPartners