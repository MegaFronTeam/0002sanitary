mixin sContact()
	// start sContact
	+b.SECTION.sContact.section#sContact&attributes(attributes)
		.container
			if block
				block
			else
				+b.section-title
					h2 Контакты
				+e.row.row
					.col-lg-8
						+e.address.ADDRESS
							+e.item
								+e.title Адрес
								+e.item-content 121099, г. Москва, 1-й Смоленский переулок, д. 9, стр.1
								+e.map-wrap
									<script type="text/javascript" charset="utf-8" async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?um=constructor%3Aefd0f5759daad23face9b9a35326ba619b9ced1b0c63f500117ed802b553b845&amp;width=100%25&amp;height=400&amp;lang=ru_RU&amp;scroll=false"></script>
					.col-lg-4
						+e.list.UL
							+e.item.LI
								+e.title Телефон
								+e.item-content(href="tel:+74992418628") 8 (499) 241 86 28
							+e.item.LI
								+e.title E-mail
								+e.item-content(href="mailto:cgon@cgon.ru") cgon@cgon.ru
							+e.item.LI
								+e.title ВКонтакте
								+e.item-content(href="vk.com/link") vk.com/link
							+e.item.LI
								+e.title Telegram
								+e.item-content(href="https://t.me/link") t.me/link
				+b.media
					+e.row.row
						+e.text-col.col-md-4.col-lg-6
							+b.section-title
								h2 Брендбук
							+e.title Полное руководство по использованию:
							ul
								li Логотипа
								li Цветов
								li Шрифтов
								li Фирменного стиля
							+e.btn.btn.btn-outline-light
								+icon('internet')
								| Открыть PDF
						.col-md-8.col-lg-6
							+e.link(href="#")
								+img('img/media.jpg')
	// end sContact